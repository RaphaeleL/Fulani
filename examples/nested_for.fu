// Nested for loop example

void main() {
    println("Nested for loops example");
    println("------------------------");
    
    // Nested for loops to create a multiplication table
    for (int i = 1; i <= 5; i = i + 1) {
        for (int j = 1; j <= 5; j = j + 1) {
            int product = i * j;
            println(i, " x ", j, " = ", product);
        }
        println("-----"); // Separator between rows
    }
} 